{
	"name": "n8n-nodes-bling-erp",
	"version": "0.1.1",
	"description": "n8n community node for Bling ERP API v3",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"bling",
		"erp",
		"nfe",
		"brasil"
	],
	"license": "MIT",
	"homepage": "https://github.com/kprprivate/bling-n8n-community-node",
	"author": {
		"name": "Marco"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/kprprivate/bling-n8n-community-node.git"
	},
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/BlingOAuth2Api.credentials.js"
		],
		"nodes": [
			"dist/nodes/Bling/Bling.node.js",
			"dist/nodes/Bling/BlingTrigger.node.js"
		]
	},
	"devDependencies": {
		"@types/node": "^25.2.1",
		"@typescript-eslint/parser": "^7.15.0",
		"eslint": "^8.56.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.1",
		"gulp": "^4.0.2",
		"n8n-workflow": "^1.0.0",
		"prettier": "^3.3.2",
		"typescript": "^5.5.3"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
